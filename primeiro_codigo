const val MAX_AGE = 20
const val MIN_AGE = 90

// Função MAIN
fun main() {
    var currentAge = 23
    val optimalAge = 19
    println("Idade Minima: " + MIN_AGE)
    println("Idade Máxima: " + MAX_AGE)
    println("Sua idade tem " + (currentAge - optimalAge) + " anos de diferença da idade ótima!")
    
}
